{
  "arrowParens": "always",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "tabWidth": 2,
        "printWidth": 100,
        "proseWrap": "always"
      }
    },
    {
      "files": "test/integration/**/*.ts",
      "options": {
        "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-packagejson"]
      }
    }
  ],
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-packagejson", "prettier-plugin-jsdoc"],
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false
}
